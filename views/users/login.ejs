<main class="mainLogIn">
    <form>
        <div class="formu">
            <label for="nombreDeUsuario">Nombre de usuario/a</label>
            <input type="text" name="nombre" id="nombreDeUsuario" minlength="4" required />
            <br />
            <label for="contrasena">Contraseña</label>
            <input type="password" name="contrasena" id="contrasena" alt="strongPass" minlength="4" required />
          <br />
          <button type="submit">Enviar</button>
          <p>Not tenés cuenta? Regrstrate <a href="/registro">acá</a></p>
        </div>
    </form>
</main>
